{
  "name": "@aureooms/js-merge-insertion-sort",
  "description": "sorting code bricks for JavaScript",
  "version": "7.0.1",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "ava": {
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "files": [
      "test/src/**/*"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "development": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "production": {
        "plugins": [
          "babel-plugin-unassert",
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-merge-insertion-sort/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@aureooms/js-array": "4.0.0",
    "@aureooms/js-in-situ-sort-spec": "8.0.0",
    "@aureooms/js-search": "0.0.4",
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/register": "7.11.5",
    "ava": "3.15.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.0.1",
    "babel-preset-power-assert": "3.0.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "6.5.0",
    "nyc": "15.1.0",
    "power-assert": "1.6.1",
    "regenerator-runtime": "0.13.7",
    "xo": "^0.33.1"
  },
  "files": [
    "lib"
  ],
  "homepage": "http://aureooms.github.io/js-merge-insertion-sort/",
  "keywords": [
    "bricks",
    "javascript",
    "js",
    "order",
    "sort",
    "sorting"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/aureooms/js-merge-insertion-sort.git",
    "type": "git"
  },
  "scripts": {
    "build": "babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "dev": "npm run lint -- --fix && npm run cover -- -- -st --fail-fast",
    "esdoc": "esdoc",
    "lint": "xo",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava",
    "travis": "npm run lint && npm run cover"
  },
  "sideEffects": false,
  "xo": {
    "prettier": true,
    "ignore": [
      "lib",
      "doc"
    ],
    "plugins": [
      "unicorn"
    ]
  }
}
